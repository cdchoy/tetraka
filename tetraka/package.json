{
  "name": "tetraka",
  "version": "1.0.0",
  "description": "An in browser tetris game",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "node server",
    "build": "tsc server/index && cd react-ui/ && npm install && npm run build",
    "tsc": "tsc server/index",
    "clean": "rm server/models/*.js; rm server/index.js"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/socket.io": "^2.1.2",
    "express": "^4.16.4",
    "socket.io": "^2.4.0",
    "typescript": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cchoy96/tetraka.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "GPL-3.0-or-later",
  "devDependencies": {}
}
